{
  "name": "metamark",
  "type": "module",
  "version": "0.8.0",
  "repository": "aegatlin/metamark",
  "types": "./dist/index.d.ts",
  "main": "./dist/index.js",
  "exports": {
    "types": "./dist/index.d.ts",
    "import": "./dist/index.js",
    "require": "./dist/index.cjs"
  },
  "scripts": {
    "ppp": "npm version patch && git push && npm publish",
